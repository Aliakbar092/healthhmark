                <div class="row" id="marketplace">
                  <% animals.forEach(animal => { %>
                    <div class="col-md-4 mb-4 animal-card"
                         data-name="<%= animal.name.toLowerCase() %>"
                         data-color="<%= animal.color.toLowerCase() %>"
                         data-price="<%= animal.price %>"
                         data-age="<%= animal.age %>"
                         data-location="<%= animal.location.toLowerCase() %>"
                         data-breed="<%= animal.breed.toLowerCase() %>">
                         
                      <div class="card anm">
                        <img src="/image/<%= animal._id %>" class="card-img-top" alt="<%= animal.name %>">
                        <div class="card-body">
                          <h5 class="card-title"><%= animal.name %></h5>
                          <p class="card-text">
                            <strong>Breed:</strong> <%= animal.breed %><br>
                            <strong>Age:</strong> <%= animal.age %> Years<br>
                            <strong>Color:</strong> <%= animal.color %><br>
                            <strong>Location:</strong> <%= animal.location %><br>
                            <strong>Price:</strong> Rs <%= animal.price %><br>
                            <strong>Stock:</strong> <%= animal.stock > 0 ? animal.stock : 'Out of Stock' %>
                          </p>
                          <button class="btn btn-primary add-to-cart" 
                              data-animal-id="<%= animal._id %>" 
                              data-animal-name="<%= animal.name %>" 
                              data-animal-price="<%= animal.price %>" 
                              data-animal-image="/image/<%= animal._id %>"
                              <%= animal.stock === 0 ? 'disabled' : '' %>>
                              Add to Cart
                          </button>
                
                          <a href="/lisitng/<%= animal._id %>">
                            <button class="btn btn-secondary">Details</button>
                          </a>
                        </div>
                      </div>
                    </div>
                  <% }) %>
                </div>
                
                
                <script>
                 // Function to filter animals
function filterAnimals() {
    const nameFilter = document.getElementById('filter-name').value.toLowerCase();
    const colorFilter = document.getElementById('filter-color').value.toLowerCase();
    const priceMin = parseFloat(document.getElementById('filter-price-min').value) || 0;
    const priceMax = parseFloat(document.getElementById('filter-price-max').value) || Infinity;
    const ageFilter = parseFloat(document.getElementById('filter-age').value) || null;
    const locationFilter = document.getElementById('filter-location').value.toLowerCase();
    const breedFilter = document.getElementById('filter-breed').value.toLowerCase();

    const animals = document.querySelectorAll('.animal-card');

    animals.forEach(animal => {
        const name = animal.getAttribute('data-name');
        const color = animal.getAttribute('data-color');
        const price = parseFloat(animal.getAttribute('data-price'));
        const age = parseFloat(animal.getAttribute('data-age'));
        const location = animal.getAttribute('data-location');
        const breed = animal.getAttribute('data-breed');

        const matchesName = !nameFilter || name.includes(nameFilter);
        const matchesColor = !colorFilter || color.includes(colorFilter);
        const matchesPrice = price >= priceMin && price <= priceMax;
        const matchesAge = !ageFilter || age === ageFilter;
        const matchesLocation = !locationFilter || location.includes(locationFilter);
        const matchesBreed = !breedFilter || breed.includes(breedFilter);

        if (matchesName && matchesColor && matchesPrice && matchesAge && matchesLocation && matchesBreed) {
            animal.style.display = '';
        } else {
            animal.style.display = 'none';
        }
    });
}

// Attach event listeners to the filters for real-time search
document.getElementById('filter-name').addEventListener('input', filterAnimals);
document.getElementById('filter-color').addEventListener('input', filterAnimals);
document.getElementById('filter-price-min').addEventListener('input', filterAnimals);
document.getElementById('filter-price-max').addEventListener('input', filterAnimals);
document.getElementById('filter-age').addEventListener('input', filterAnimals);
document.getElementById('filter-location').addEventListener('input', filterAnimals);
document.getElementById('filter-breed').addEventListener('input', filterAnimals);


