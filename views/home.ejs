// On document load, initialize the cart count from localStorage
document.addEventListener('DOMContentLoaded', function() {
    updateCartIcon();
});

document.querySelectorAll('.add-to-cart').forEach(button => {
    button.addEventListener('click', function() {
        const cart = JSON.parse(localStorage.getItem('cart')) || [];
        
        const animalId = button.getAttribute('data-animal-id');
        const animalName = button.getAttribute('data-animal-name');
        const animalPrice = parseFloat(button.getAttribute('data-animal-price'));
        const animalImage = button.getAttribute('data-animal-image');

        // Check if the item already exists in the cart
        const existingItem = cart.find(item => item._id === animalId);

        if (existingItem) {
            // Increase quantity if the item already exists in the cart
            alert(animalName + " is already in the cart.");
        } else {
            // Add the new item with quantity set to 1
            cart.push({
                _id: animalId,
                name: animalName,
                price: animalPrice,
                image: animalImage,
                quantity: 1
            });
            localStorage.setItem('cart', JSON.stringify(cart));
            alert(`${animalName} added to cart!`);
            updateCartIcon();
        }

    });
});

