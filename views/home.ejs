<script>
  function searchAnimals() {
  const input = document.getElementById("animalSearch").value.toLowerCase();
  const animals = document.querySelectorAll("#marketplace .col-md-4");

  animals.forEach((animal) => {
    const name = animal.querySelector(".card-title").textContent.toLowerCase();
    const breed = animal.querySelector(".card-text").textContent.toLowerCase();

    if (name.includes(input) || breed.includes(input)) {
      animal.style.display = "block";
    } else {
      animal.style.display = "none";
    }
  });
}

document.getElementById("searchIcon").addEventListener("click", function() {
  const searchBar = document.getElementById("searchBar");
  searchBar.style.display = searchBar.style.display === "none" ? "block" : "none";
});

